{
    "name": "@remitly/google-libphonenumber",
    "version": "1.0.0",
    "description": "Remitly's fork of a phone number validation package",
    "author": "Remitly, Inc.",
    "license": "UNLICENSED",
    "homepage": "https://github.com/Remitly/google-libphonenumber",
    "bugs": "https://github.com/Remitly/google-libphonenumber/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Remitly/orca-icon-font.git"
    },
    "main": "dist/libphonenumber.js",
    "keywords": [
        "browserify",
        "e164",
        "format",
        "formatting",
        "international",
        "libphonenumber",
        "number",
        "phone",
        "phonenumber",
        "rfc3966",
        "standardize",
        "wrapper"
    ],
    "options": {
        "mocha": "--require should test"
    },
    "scripts": {
        "build": "bin/build.sh",
        "changelog": "github_changelog_generator --no-issues --header-label='# Changelog' --future-release=v$npm_config_future_release && sed -i '' -e :a -e '$d;N;2,3ba' -e 'P;D' CHANGELOG.md",
        "lint": "eslint src/index.js test",
        "prepare": "bin/prepare.sh",
        "prepublish": "npm run build",
        "test": "npm run build && npm run testonly",
        "testonly": "mocha $npm_package_options_mocha",
        "update": "bin/update.sh"
    },
    "devDependencies": {
        "browserify": "^13.1.0",
        "eslint": "^3.5.0",
        "eslint-config-seegno": "^6.0.0",
        "mocha": "^3.0.2",
        "should": "^11.1.0"
    },
    "engines": {
        "node": ">=0.10"
    },
    "publishConfig": {
        "registry": "https://npm.int.remitly.com/"
    }
}
